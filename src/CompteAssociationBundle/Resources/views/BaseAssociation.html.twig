<script>
    $(document).ready(function(){
        $(function(){
            affichage();
            affichageassoc();
        });
    });
    function affichage()
    {
        $(function(){
            $.ajax({
                type: 'GET',
                url: '{{ path('membre_affichagAjax') }}',
                dataType: 'text',
                success: function(data) {


                    if(data >0)
                    {
                        $('#groupe').append( '<a class="nav-link "  href="{{ path('liste-groupe') }}"><i class="fa fa-group"></i> Groupe</a>' );

                    }

                },
                error: function() {
                    alert('La requête n\'a pas abouti');
                }

            });
        });


    }
    function affichageassoc()
    {
        $(function(){
            $.ajax({
                type: 'GET',
                url: '{{ path('membreassoc_affichagAjax') }}',
                dataType: 'text',
                success: function(data) {


                    if(data >0)
                    {
                        $('#association').append( '<a class="nav-link "  href="{{ path('liste-association') }}"><i class="fa fa-sitemap"></i> Association</a>' );

                    }

                },
                error: function() {
                    alert('La requête n\'a pas abouti');
                }

            });
        });


    }
</script>
